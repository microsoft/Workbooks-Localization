      <Item ItemId=";items.groupSAPOverview.content.items.textComputeOverview.content.json" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[### Compute Group]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
