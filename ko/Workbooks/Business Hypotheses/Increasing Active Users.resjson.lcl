<LCX SchemaVersion="6.0" Name="S:\output\loc\Business Hypotheses\Increasing Active Users.resjson" PsrId="306" FileType="1" SrcCul="en-US" TgtCul="ko-KR" xmlns="http://schemas.microsoft.com/locstudio/2006/6/lcx">
