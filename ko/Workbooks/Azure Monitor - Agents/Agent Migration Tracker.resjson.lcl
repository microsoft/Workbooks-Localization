      <Item ItemId=";items.DCRGroup.content.items.dcrheader.content.json" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[# Data Collection Rules]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
