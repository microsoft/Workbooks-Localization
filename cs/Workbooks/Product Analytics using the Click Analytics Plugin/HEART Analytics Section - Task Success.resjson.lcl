      <Item ItemId=";items.group0.content.items.TaskSuccessMetrics.content.items.TaskCompletionTitle.content.json" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[#### Task Completion]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
