      <Item ItemId=";items.Databases.content.items.MySQL.content.items.kqldatabasemysqlsingleresourcesdetails.content.title" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[MySQL - Single]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
