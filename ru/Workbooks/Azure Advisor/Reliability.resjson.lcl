      <Item ItemId=";items.Databases.content.items.Cosmos.content.items.kqldatabasecosmosdbresourcesdetails.content.title" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Cosmos DB]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[Cosmos DB]]></Val>
          </Tgt>
