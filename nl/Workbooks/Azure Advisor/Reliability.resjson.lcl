      <Item ItemId=";items.Compute.content.items.links5.content.links.db54cc9de67e4687b9e6e73aefb9638c.linkLabel" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Virtual Machine Scale Sets]]></Val>
