      <Item ItemId=";items.group1.content.items.query5.content.title" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Per Second Shard Usage Snapshot]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
