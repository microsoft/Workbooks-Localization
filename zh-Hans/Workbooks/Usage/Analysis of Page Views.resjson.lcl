      <Item ItemId=";items.pageexitgroup.content.items.PageExitRatesgrid.content.title" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[Page Exit Rate by Page Name]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
