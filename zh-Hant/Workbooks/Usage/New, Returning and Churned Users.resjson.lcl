          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[分析的時間範圍]]></Val>
          </Tgt>
        </Str>
        <Disp Icon="Str" />
      </Item>
      <Item ItemId=";items.1.content.parameters.f5554bff7f79444a80735f9592474a0c.description" ItemType="0" PsrId="306" Leaf="true">
