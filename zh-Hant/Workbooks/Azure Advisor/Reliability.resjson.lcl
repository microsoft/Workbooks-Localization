      <Item ItemId=";items.Web.content.items.AppServicePlan.content.items.AppServicePlanquery.content.title" ItemType="0" PsrId="306" Leaf="true">
        <Str Cat="Text">
          <Val><![CDATA[App Service Plan]]></Val>
          <Tgt Cat="Text" Stat="Loc" Orig="New">
            <Val><![CDATA[App Service 方案]]></Val>
